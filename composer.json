{
  "name": "wambo/wambo-composer-installer",
  "description": "A composer plugin to install wambo modules to a wambo project",
  "type": "composer-plugin",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Rene Penner",
      "email": "rene@penner.name",
      "role": "Developer"
    },
    {
      "name": "Andy Koch",
      "email": "andyk7@gmail.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Wambo\\ComposerInstaller\\": "src/Wambo/ComposerInstaller/"
    }
  },
  "extra": {
    "class": "Wambo\\ComposerInstaller\\Plugin"
  },
  "require": {
    "wambo/module-core": "dev-feature/base",
    "composer-plugin-api": "^1.0"
  },
  "require-dev": {
    "Composer/Composer": "^1.0",
    "phpunit/phpunit": "5.4.*",
    "squizlabs/php_codesniffer": "2.*",
    "phpmd/phpmd" : "@stable"
  }
}
